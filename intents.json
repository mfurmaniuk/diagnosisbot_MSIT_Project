{
    "intents": 
    [
        {"tag": "greeting",
         "patterns": ["Hi", "How are you", "Is anyone there?", "Hello", "Good day", "Whats up"],
         "responses": ["Hello!", "Good to see you again!", "Hi there, how can I help?"],
         "context_set": "salutations"
        },
        {"tag": "goodbye",
         "patterns": ["cya", "See you later", "Goodbye", "I am leaving", "Have a good day", "bye"],
         "responses": ["Sad to see you go :(", "Talk to you later", "Goodbye!"],
         "context_set": "sayonara"
        },
        {"tag": "chatbot",
         "patterns": ["What are you?", "are you a chatbot", "what is a chatbot", "chatbot?"],
         "responses": ["I am an AI powered chatbot!", "I am here to see how sick you are"],
         "context_set": "chatbot"
        },
        {"tag": "name",
         "patterns": ["what is your name", "what should I call you", "whats your name?"],
         "responses": ["You can call me Chatbot.", "I'm Chatbot!", "I'm Chatbot."],
         "context_set": "who"
        },
        {"tag": "sick",
         "patterns": ["Im feeling sick", "not feeling well", "Im coming down with something", "I am ill"],
         "responses": ["Sorry to hear you are not feeling well", "What kind of symptoms do you have?"],
         "context_set": "symptom"
        },
        {"tag": "doctor",
         "patterns": ["I may need a doctor", "want to see a doctor", "I need a physical"],
         "responses": ["Sounds like you are ill", "A doctor can help diagnose you", "Perhaps I can tell you what is wrong"],
         "context_set": "physician"
        },
        {"tag": "hospital",
         "patterns": ["I need to go to the hospital", "Maybe I should go to the emergency room", "go to the hospital"],
         "responses": ["A hospital?! Sounds serious.", "You must be really sick"],
         "context_set": "hospital"
        },
        {"tag": "symptom_info",
         "patterns": ["Tell me about {SymptomName}","What is {SymptomName}?","Explain {SymptomName}","Information about {SymptomName}"],
         "responses": ["Fetching information about {SymptomName}."],
         "context_set": "symptom"
        },
        {"tag": "disease_info",
         "patterns": ["Tell me about {DiseaseName}","What is {DiseaseName}?","Explain {DiseaseName}","Information about {DiseaseName}"],
         "responses": ["Fetching information about {DiseaseName}."],
         "context_set": "disease"
        }
   ]
}